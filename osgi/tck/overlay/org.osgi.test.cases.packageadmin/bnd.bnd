Bundle-Version						: 4.2.0.${project.build}
Bundle-Description					: Framework test cases.

Private-Package						= org.osgi.test.support.*, \
	${p}.junit.*

Export-Package						= \


Include-Resource = \
	tc1.tb1.jar, \
	tc1.tb2.jar, \
	tc1.tb3.jar, \
	tc2.tb1.jar, \
	tc2.tb2.jar, \
	tc2.tb3.jar, \
	tc2.tb4.jar, \
	tc2.tb5.jar, \
	tc3.tb1.jar, \
	tc3.tb2.jar, \
	tc3.tb3.jar, \
	tc5.tb1.jar, \
	tc5.tb2.jar, \
	tc5.tb3.jar, \
	tc5.tb4.jar, \
	tc5.tb5.jar, \
	tc5.tb6.jar
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.packageadmin

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.service.packageadmin;			version=project	, \
	osgi.core;								version=4.2	, \
	ee.minimum; 							version=1.2	, \
	${junit}

-runbundles							= \
	

-runproperties = report=true, \
jboss.home="@jboss-as-dir@/build/target/@jboss-as-buildname@", \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict
