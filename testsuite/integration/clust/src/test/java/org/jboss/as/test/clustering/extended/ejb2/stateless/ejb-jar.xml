                         "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
	<enterprise-beans>
		<session>
        	<ejb-name>StatelessBean</ejb-name>
         	<home>org.jboss.as.test.clustering.extended.ejb2.stateless.StatelessRemoteHome</home>
         	<remote>org.jboss.as.test.clustering.extended.ejb2.stateless.StatelessRemote</remote>
         	<ejb-class>org.jboss.as.test.clustering.extended.ejb2.stateless.StatelessBeanDD</ejb-class>
