<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
  
  <bean id="beanA" class="org.jboss.as.testsuite.integration.osgi.blueprint.bundle.BeanA">
    <property name="mbeanServer" ref="mbeanService"/>
  </bean>
  
  <service id="serviceA" ref="beanA" interface="org.jboss.as.testsuite.integration.osgi.blueprint.bundle.ServiceA">
  </service>
  
  <service id="serviceB" interface="org.jboss.as.testsuite.integration.osgi.blueprint.bundle.ServiceB">
    <bean class="org.jboss.as.testsuite.integration.osgi.blueprint.bundle.BeanB">
       <property name="beanA" ref="beanA"/>
    </bean>
  </service>
  
  <reference id="mbeanService" interface="javax.management.MBeanServer"/>

</blueprint>