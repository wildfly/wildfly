		</property>
		<property name="Integer">
			<value>1</value>
		</property>
		<property name="Long">
			<value>1</value>
		</property>
		<property name="Float">
			<value>1</value>
		</property>
		<property name="Double">
			<value>1</value>
		</property>
		<property name="AtomicBoolean">
			<value>true</value>
		</property>
		<property name="AtomicInteger">
			<value>3</value>
		</property>
		<property name="AtomicLong">
			<value>2</value>
		</property>
		<property name="BigDecimal">
			<value>10000000</value>
		</property>
