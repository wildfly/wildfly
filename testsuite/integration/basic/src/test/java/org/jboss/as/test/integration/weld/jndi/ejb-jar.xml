<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <session>
            <ejb-name>StartupSingletonEjb</ejb-name>
            <ejb-class>org.jboss.as.test.integration.weld.jndi.StartupSingletonEjb</ejb-class>
            <env-entry>
                <env-entry-name>java:global/foo</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>foo-value</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>


