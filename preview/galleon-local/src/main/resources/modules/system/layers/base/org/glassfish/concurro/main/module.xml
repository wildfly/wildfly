<module xmlns="urn:jboss:module:1.9" name="org.glassfish.concurro">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${org.glassfish.concurro:concurro}"/>
    </resources>

    <dependencies>
        <module name="java.logging"/>
        <module name="java.management"/>
        <module name="java.naming"/>
        <module name="jakarta.enterprise.concurrent.api"/>
        <module name="jakarta.enterprise.api"/>
        <module name="jakarta.interceptor.api"/>
        <module name="jakarta.transaction.api" optional="true"/>
    </dependencies>
</module>
