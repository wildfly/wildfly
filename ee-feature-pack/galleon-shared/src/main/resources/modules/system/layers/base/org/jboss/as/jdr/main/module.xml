<module xmlns="urn:jboss:module:1.9" name="org.jboss.as.jdr">

    <properties>
        <property name="jboss.api" value="private"/>
        <property name="jboss.require-java-version" value="17"/>
    </properties>

    <main-class name="org.jboss.as.jdr.CommandLineMain"/>

    <exports>
        <exclude path="org/jboss/as/jdr/logger"/>
    </exports>

    <resources>
        <artifact name="${org.wildfly:wildfly-jdr}"/>
        <resource-root path="resources"/>
    </resources>

    <dependencies>
        <module name="java.management"/>
        <module name="org.apache.commons.cli"/>
        <module name="org.jboss.staxmapper"/>
        <module name="org.jboss.as.controller"/>
        <module name="org.jboss.as.network"/>
        <module name="org.wildfly.security.elytron-private"/>
        <module name="org.jboss.as.server"/>
        <module name="org.jboss.as.cli"/>
        <module name="org.jboss.modules"/>
        <module name="org.jboss.threads"/>
        <module name="org.jboss.msc"/>
        <module name="org.jboss.logging"/>
        <module name="org.jboss.logmanager" services="import"/>
        <module name="org.jboss.vfs"/>
        <module name="org.wildfly.common"/>
        <module name="java.xml"/>
    </dependencies>
</module>
