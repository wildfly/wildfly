					<xs:element name="abstract" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								This is a short summary of the Solution
								suitable for display as part of search results. If a particular
								solution
								does not have this field set; then one is automatically
								created by Calaveras.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="extract" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								This field is only set when the solution is
								part of search results (or recommendations). This is an excerpt
								from
								the solution which contains the section(s) of the solution
								that best indicate why this solution matched the
								search query.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="explanation" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Lucene search result explanation of why and how well this document matches the search query.
								This field is only set when the solution is part of search results (or
								recommendations).  This field only is present when the 'explain=true' search query
								parameter is used.  This field is only for debugging purposes.
								See 'extract' for a more generally useful field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="detectedLanguage" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Only for solutions returned from Calaveras.
								Language of the solution detected by Google's language-detection.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="setLanguage" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Only for solutions returned from Calaveras.
								The language as set by the author in drupal.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="indexedDate" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Time when the solution has been indexed.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="accessState" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The access state in the KCS lifecycle the solution is currently in. The possible states are "active","private", "retired".
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="solutionsType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                    A list of solutions.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>
			<xs:documentation>A list of solutions.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element ref="tns:solution" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="supportNeedType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                    A support need.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>
			<xs:documentation>A support need.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:trackedEntityType">
				<xs:sequence>
					<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case identifier assigned by backing system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>RESTful URI for this support need resource.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="summary" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Title or short summary. This field is required by the model but the schema allows it to be
								omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Description of the problem or question. This field is required by the model
								but the schema allows it to be omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="status" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation> Case status. Valid values are available from the values service.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Product this case relates to. Valid products are available from the products resource. This
								field is required by the model but the schema allows it to be omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="component" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Component this case relates to (optional).  Valid components for a given product are available
								from the values service.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="version" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Version of the product this case relates to.  Valid versions for a given product are available
								from the products resource.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation> Type of case, i.e. question, problem, etc.  Valid types are available from the values service.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="accountNumber" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Account number this case belongs to. This field is required by the model but the schema allows it to be
								omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="view_uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Portal URI for this solution.  </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reference" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="notes" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="escalated" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="contactName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="contactSsoUsername" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="origin" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="owner" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="internalPriority" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="internalStatus" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="suppliedName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="suppliedPhone" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="suppliedEmail" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="severity" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="folderNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="tns:comments" minOccurs="0"/>
					<xs:element ref="tns:notified_users" minOccurs="0"/>
					<xs:element ref="tns:entitlement" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="tns:solutions" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>

				<xs:attribute name="alternateId" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- ============================================================= -->

	<xs:complexType name="sqiRatingType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                    An SQI Rating for a KCS Solution.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>
			<xs:documentation>An SQI Rating for a KCS Solution.</xs:documentation>
		</xs:annotation>

		<xs:complexContent>
			<xs:extension base="tns:trackedEntityType">
				<xs:sequence>
					<xs:element name="solution" type="tns:sqiSolutionLinkType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A link to the Solution this sqiRating is applicable to.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="completeTitle" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is the title complete?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="adheresToContentStandard" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Solution adheres to current template, style,
								and formatting guidelines in the content
								standard?
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="accuratelyReflectsProblem" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Does the issue accurately reflect the
								customer's problem and context?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="isUnique" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is article unique?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="clearAndConcise" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is content clear and concise?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="metadataSetCorrectly" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is article metadata set correctly?</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="sqiSolutionLinkType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                    A link to the Solution this sqiRating is applicable to.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>
			<xs:documentation>A link to the Solution this sqiRating is applicable to.</xs:documentation>
		</xs:annotation>

		<xs:simpleContent>
			<xs:extension base="xs:anyURI">
				<xs:attribute name="rel" type="xs:string" use="optional" default="solution"/>
				<xs:attribute name="solutionId" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="tagType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                    Complex Type for a Tag.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>
			<xs:documentation>Complex Type for a Tag.</xs:documentation>
		</xs:annotation>            
		<xs:sequence>
			<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ================================================================================================= -->

	<xs:element name="article" type="tns:articleType">
		<xs:annotation>
			<xs:documentation>
				A KCS Article. Some elements have subelements called "html" and "text". The "html" field
				is "safe value" field. It is the text field rendered as HTML so there is no markup in the
				"html" field and it is not editable. The "text" field is an editable
				field that may contain HTML markup.
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="articles" type="tns:articlesType">
		<xs:annotation>
			<xs:documentation>A list of articles.</xs:documentation>
		</xs:annotation>
	</xs:element>

    <xs:element name="cve" type="tns:cveType">
        <xs:annotation>
            <xs:documentation>CVE.</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="cves" type="tns:cvesType">
        <xs:annotation>
            <xs:documentation>CVEs.</xs:documentation>
        </xs:annotation>
    </xs:element>

	<xs:element name="knowledgeDescription" type="tns:knowledgeDescriptionType">
		<xs:annotation>
			<xs:documentation>A description of the something knowledge related like a solution issue or environment.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="privateNotes" type="tns:privateNotesType">
		<xs:annotation>
			<xs:documentation>Private notes section</xs:documentation>
		</xs:annotation>                
	</xs:element>

	<xs:element name="recommendation" type="tns:recommendationType">
		<xs:annotation>
			<xs:documentation>A suggestion referring to a solution and related meta data.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="recommendations" type="tns:recommendationsType">
		<xs:annotation>
			<xs:documentation>A list of suggestions.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="solution" type="tns:solutionType">
		<xs:annotation>
			<xs:documentation>
				A KCS Solution. Some elements have subelements called "html" and "text". The "html" field
				is a "safe value" field.  It is the text field containing HTML markup rendered from the
				markdown stored in "value". The "html" field is not editable. The
				"text" field is the editable field that may contain markdown.
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="solutions" type="tns:solutionsType">
		<xs:annotation>
			<xs:documentation>A list of solutions.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="supportNeed" type="tns:supportNeedType">
		<xs:annotation>
			<xs:documentation>A support need.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="tag" type="tns:tagType"/>

	<xs:element name="tags" type="tns:tagsType">
		<xs:annotation>
			<xs:documentation>A list of Tags.</xs:documentation>        
		</xs:annotation>
	</xs:element>

</xs:schema>
