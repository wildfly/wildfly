<?xml version="1.0" encoding="UTF-8"?>
<feature-group-spec name="domain" xmlns="urn:jboss:galleon:feature-group:1.0">

    <origin name="org.wildfly:wildfly-ee-galleon-pack">
        <feature-group name="domain"/>
    </origin>

    <feature spec="profile">
        <param name="profile" value="default"/>
        <feature spec="subsystem.microprofile-jwt-smallrye"/>
        <feature-group name="microprofile-opentracing-jaeger"/>
    </feature>
    <feature-group name="domain-host-excludes"/>

</feature-group-spec>
