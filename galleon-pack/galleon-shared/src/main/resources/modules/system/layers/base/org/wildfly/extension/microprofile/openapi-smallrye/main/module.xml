<module xmlns="urn:jboss:module:1.9" name="org.wildfly.extension.microprofile.openapi-smallrye">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <exports>
        <exclude path="org/wildfly/extension/microprofile/openapi/logging"/>
    </exports>

    <resources>
        <artifact name="${org.wildfly:wildfly-microprofile-openapi-smallrye-extension}"/>
    </resources>

    <dependencies>
        <module name="io.smallrye.jandex"/>
        <module name="io.smallrye.openapi"/>

        <module name="io.undertow.core"/>
        <module name="io.undertow.servlet"/>

        <module name="org.eclipse.microprofile.config.api"/>
        <module name="org.eclipse.microprofile.openapi.api"/>

        <module name="org.jboss.as.controller"/>
        <module name="org.jboss.as.ee"/>
        <module name="org.jboss.as.jaxrs"/>
        <module name="org.jboss.as.network"/>
        <module name="org.jboss.as.server"/>
        <module name="org.jboss.as.web-common"/>
        <module name="org.jboss.logging"/>
        <module name="org.jboss.metadata.common"/>
        <module name="org.jboss.metadata.web"/>
        <module name="org.jboss.modules"/>
        <module name="org.jboss.msc"/>
        <module name="org.jboss.staxmapper"/>
        <module name="org.jboss.vfs"/>

        <module name="org.wildfly.common"/>
        <module name="org.wildfly.extension.undertow"/>
        <module name="org.wildfly.microprofile.openapi-smallrye.host"/>
        <module name="org.wildfly.microprofile.openapi-smallrye.spi"/>
        <module name="org.wildfly.security.elytron-private"/>
        <module name="org.wildfly.service"/>
        <module name="org.wildfly.subsystem"/>
    </dependencies>
</module>
