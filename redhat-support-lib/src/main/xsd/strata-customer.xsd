	<xs:complexType name="caseType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                     A support case.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>                       
			<xs:documentation>A support case.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:trackedEntityType">
				<xs:sequence>
					<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Case identifier assigned by backing system.  </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>RESTful URI for this case resource.  </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="summary" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Title or short summary. This field is required by the model but the schema allows it to be
								omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Description of the problem or question. This field is required by the model
								but the schema allows it to be omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="status" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Case status. Valid values are available from the values service.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Product this case relates to. Valid products are available from the products resource. This
								field is required by the model but the schema allows it to be omitted so that
								field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="component" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Component this case relates to (optional).  Valid components for a given product are available
								from the values service.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="version" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Version of the product this case relates to.  Valid versions for a given product are available
								from the products resource.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation> Type of case, i.e. question, problem, etc.  Valid types are available from the values service.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="accountNumber" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Account number this case belongs to. This field is required by the model but the schema allows
								it to be omitted so that field-level updates are possible.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="view_uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Portal URI for this solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="reference" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="notes" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="escalated" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="contactName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="contactSsoUsername" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="origin" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="owner" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="internalPriority" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="internalStatus" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="suppliedName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="suppliedPhone" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="suppliedEmail" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="severity" type="xs:string" minOccurs="0" maxOccurs="1"/>

					<xs:element name="tags" type="tns:tagsType">
						<xs:annotation>
							<xs:documentation>TAGs for this case.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="folderNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="tns:comments" minOccurs="0"/>
					<xs:element ref="tns:notified_users" minOccurs="0"/>
					<xs:element ref="tns:entitlement" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="tns:recommendations" minOccurs="0" maxOccurs="1"/>
					<xs:element name="fts" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="tns:bugzillas" minOccurs="0" maxOccurs="1"/>

					<xs:element name="explanation" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Lucene search result explanation of why and how well this document
								matches the search query.  This field is only set when the case is part of search results
								(or recommendations).  This field only is present when the 'explain=true' search query
								parameter is used.  This field is only for debugging purposes.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>

				<xs:attribute name="caseNumber" type="xs:string" use="optional"/>
				<xs:attribute name="closed" type="xs:boolean" use="optional"/>
				<xs:attribute name="alternateId" type="xs:string" use="optional"/>
				<xs:attribute name="contactInfo24x7" type="xs:string" use="optional"/>					
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="casesType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                    A list of support cases. This type serves as both a top-level construct in the case that a query
                                    returns a list of support cases as well as an embedded construct as part of a larger
                                    item. This element is not required to contain any child case elements.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>                    
			<xs:documentation>
				A list of support cases. This type serves as both a top-level construct in the case that a query
				returns a list of support cases as well as an embedded construct as part of a larger
				item. This element is not required to contain any child case elements.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:strataEntityType">
				<xs:sequence>
					<xs:element ref="tns:case" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="previous" type="xs:anyURI" minOccurs="0" maxOccurs="1"/>
					<xs:element name="next" type="xs:anyURI" minOccurs="0" maxOccurs="1"/>
					<xs:element name="totalCount" type="xs:int" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

