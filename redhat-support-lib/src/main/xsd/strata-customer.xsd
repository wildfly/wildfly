	<xs:complexType name="caseFilterType">
		<xs:annotation>
                        <xs:appinfo>
                            <jaxb:class>
                                <jaxb:javadoc>
                                     Complex Type for filtering cases.
                                </jaxb:javadoc>
                            </jaxb:class>
                        </xs:appinfo>
			<xs:documentation>Complex Type for filtering cases.</xs:documentation>
		</xs:annotation>            
		<xs:sequence>
			<xs:element name="endDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			<xs:element name="accountNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="includeClosed" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="tns:groupNumbers"/>
			<xs:element name="includePrivate" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="keyword" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="count" type="xs:int" minOccurs="0" maxOccurs="1"/>
			<xs:element name="start" type="xs:int" minOccurs="0" maxOccurs="1"/>
			<xs:element name="onlyUngrouped" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="ownerSSOName" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="severity" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="sortField" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="sortOrder" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="startDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			<xs:element name="status" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="createdBySSOName" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="resourceType" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="uri" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="viewURI" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="associateSSOName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						We need to provide associateSSOName to get the cases for a particular associate,If it's not provided then it will set to the calling contact. 
					</xs:documentation>
				</xs:annotation>
			</xs:element>	
			<xs:element name="contributor" type="xs:boolean" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The contributor field will be used to get the cases where the associate is the contributor.
						</xs:documentation>
					</xs:annotation>
			</xs:element>
			<xs:element name="ftsRole" type="xs:boolean" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The ftsRole field will be used to get the cases where the associate is the  fts follower.
						</xs:documentation>
					</xs:annotation>
			</xs:element>
			<xs:element name="translator" type="xs:boolean" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The translator field will be used to get the cases where the associate is the translator.
						</xs:documentation>
					</xs:annotation>
			</xs:element>
			<xs:element name="owner" type="xs:boolean" minOccurs="0" maxOccurs="1">
				 <xs:annotation>
						<xs:documentation>
							The owner field will be used to get the cases where the associate is the owner.
						</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="view" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
						<xs:documentation>
							The value for the field should be pass as "internal" to get the response from InternalAPI.
						</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="bugzillaType">
