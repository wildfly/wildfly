<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- This document is used by JAXB to generate Java classes at project build 
	time. Most entities can be the top-level payload of an REST API response 
	or embedded in another entity. -->
<xs:schema targetNamespace="http://www.redhat.com/gss/strata"
	elementFormDefault="qualified" version="1.14.0-SNAPSHOT-SNAPSHOT"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:tns="http://www.redhat.com/gss/strata"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	jaxb:version="2.0" jaxb:extensionBindingPrefixes="xjc">

	<xs:annotation>
		<xs:documentation>Red Hat Customer Portal API</xs:documentation>
		<xs:appinfo>
			<jaxb:globalBindings>
				<xjc:serializable uid="11"/>
				<jaxb:javaType xmlType="xs:dateTime" name="java.util.Calendar"
					parseMethod="javax.xml.bind.DatatypeConverter.parseDateTime"
					printMethod="javax.xml.bind.DatatypeConverter.printDateTime"/>
				<!-- Global binding for xs to java boolean to allow case insensitivity -->
				<jaxb:javaType xmlType="xs:boolean" name="boolean"
					parseMethod="com.redhat.gss.avalon.strata.rest.BooleanDatatypeConverter.parseBoolean"/>
			</jaxb:globalBindings>
		</xs:appinfo>
	</xs:annotation>

	<xs:complexType name="strataEntityType" abstract="true">
		<xs:annotation>
			<xs:documentation>The base type for all API entities.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="label" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						This is a repeat of the containing element name for JSON,
						since JSON doesn't include this for top level elements.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="trackedEntityType" abstract="true">
		<xs:annotation>
			<xs:documentation>The base type for entities that maintain create and update information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:strataEntityType">
				<xs:sequence>
					<xs:element name="createdBy" type="xs:string"   minOccurs="0" maxOccurs="1"/>
					<xs:element name="createdDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="lastModifiedBy" type="xs:string"   minOccurs="0" maxOccurs="1"/>
					<xs:element name="lastModifiedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="linked" type="xs:boolean"  minOccurs="0" maxOccurs="1"/>
					<xs:element name="linkedBy" type="xs:string"   minOccurs="0" maxOccurs="1"/>	
					<xs:element name="linkedAt" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>			
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="base_linkType">
		<xs:annotation>
			<xs:documentation>ATOM link type.</xs:documentation>
		</xs:annotation>

		<xs:complexContent>
			<xs:extension base="tns:strataEntityType">
				<xs:sequence>
					<xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>

				<xs:attribute name="title" type="xs:string" use="optional"/>
				<xs:attribute name="rel" type="xs:string" use="optional"/>
				<xs:attribute name="href" type="xs:string" use="optional"/>
				<xs:attribute name="type" type="xs:string" use="optional"/>
				<!-- This is not standard and is for backwards compatibility only -->
				<xs:attribute name="uri" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="problemsType">
		<xs:annotation>
			<xs:documentation>A list of problem reports.</xs:documentation>
		</xs:annotation>

		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="source" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="tns:link"/>
			<xs:element ref="tns:problem"/>
		</xs:choice>
	</xs:complexType>

	<xs:element name="problems" type="tns:problemsType">
		<xs:annotation>
			<xs:documentation>A list of problem reports.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="problemType">
		<xs:annotation>
			<xs:documentation>A problem report.</xs:documentation>
		</xs:annotation>

		<xs:sequence>    
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="source" type="xs:string" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="tns:link"/>                                
			</xs:choice>

			<xs:element ref="tns:explainSbr" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="problem" type="tns:problemType">
		<xs:annotation>
			<xs:documentation>A problem report.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="explainSbrType">
		<xs:sequence>
			<xs:element ref="tns:sbrs"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="explainSbr" type="tns:explainSbrType"/>

	<xs:complexType name="sbrsType">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="tns:sbr"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="sbrs" type="tns:sbrsType"/>

	<xs:complexType name="sbrType">
		<xs:sequence>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="score" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="sbr" type="tns:sbrType"/>

	<xs:complexType name="solutions">
		<xs:annotation>
			<xs:documentation>A list of solutions.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element ref="tns:solution" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="solutions" type="tns:solutions">
		<xs:annotation>
			<xs:documentation>A list of solutions.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="articlesType">
		<xs:annotation>
			<xs:documentation>A list of articles.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element ref="tns:article" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="articles" type="tns:articlesType">
		<xs:annotation>
			<xs:documentation>A list of articles.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="sqiSolutionLinkType">
		<xs:annotation>
			<xs:documentation>A link to the Solution this sqiRating is applicable to.</xs:documentation>
		</xs:annotation>

		<xs:simpleContent>
			<xs:extension base="xs:anyURI">
				<xs:attribute name="rel" type="xs:string" use="optional" default="solution"/>
				<xs:attribute name="solutionId" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="sqiRatingType">
		<xs:annotation>
			<xs:documentation>An SQI Rating for a KCS Solution.</xs:documentation>
		</xs:annotation>

		<xs:complexContent>
			<xs:extension base="tns:trackedEntityType">
				<xs:sequence>
					<xs:element name="solution" type="tns:sqiSolutionLinkType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A link to the Solution this sqiRating is applicable to.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="completeTitle" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is the title complete?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="adheresToContentStandard" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Solution adheres to current template, style,
								and formatting guidelines in the content
								standard?
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="accuratelyReflectsProblem" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Does the issue accurately reflect the
								customer's problem and context?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="isUnique" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is article unique?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="clearAndConcise" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is content clear and concise?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="metadataSetCorrectly" type="xs:integer" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is article metadata set correctly?</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="sqiRating" type="tns:sqiRatingType">
		<xs:annotation>
			<xs:documentation>An SQI Rating for a KCS Solution.</xs:documentation>
		</xs:annotation>
	</xs:element>



	<xs:element name="solution" type="tns:solutionType">
		<xs:annotation>
			<xs:documentation>
				A KCS Solution. Some elements have subelements called "html" and "text". The "html" field
				is a "safe value" field.  It is the text field containing HTML markup rendered from the
				markdown stored in "value". The "html" field is not editable. The
				"text" field is the editable field that may contain markdown.
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="knowledgeDescriptionType">
		<xs:annotation>
			<xs:documentation>A description of the something knowledge related like a solution issue or environment.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="text" type="xs:string"/>
			<xs:element name="html" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="knowledgeDuplicateLinkType">
		<xs:annotation>
			<xs:documentation>Links to duplicates of the solution </xs:documentation>
		</xs:annotation>

		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="rel" type="xs:string" use="optional" default="solution"/>
				<xs:attribute name="solutionId" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="knowledgeSbrsType">
		<xs:annotation>
			<xs:documentation>SBRs for this solution resource.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="sbr" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="tagsType">
		<xs:annotation>
			<xs:documentation>TAGs for this solution resource.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="tag" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="knowledgeProductsType">
		<xs:annotation>
			<xs:documentation>Drupal products list for this solution resource.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="knowledgeLinkedProductsType">
		<xs:annotation>
			<xs:documentation>
				linked products list for this solution Sresource. This list is calculated from a Math, that 
				calculatges if linked case's product can be quanitatively make to this list.
			</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="knowledgeProductFamilyType">
		<xs:annotation>
			<xs:documentation>This is the families of products to which the solution belongs to.</xs:documentation>
		</xs:annotation>

		<xs:sequence>
			<xs:element name="family" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="solutionType">
		<xs:annotation>
			<xs:documentation>
				A KCS Solution. Some elements have subelements called "html" and "text". The "html" field
				is a "safe value" field.  It is the text field containing HTML markup rendered from the
				markdown stored in "value". The "html" field is not editable. The
				"text" field is the editable field that may contain markdown.
			</xs:documentation>
		</xs:annotation>

		<xs:complexContent>
			<xs:extension base="tns:trackedEntityType">
				<xs:sequence>
					<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Solution identifier assigned by backing system.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="tnid" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Translated Solution identifier assigned by backing system.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>RESTful URI for this solution resource.  </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="uidName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Author name.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="view_uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Portal URI for this solution resource.  </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The title of the solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="authorSSOName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The SSO username of the user that authored the solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="lastModifiedBySSOName" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The SSO username of the user that last modified the solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="language" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The language the solution is written in. When a solution is
								retrieved from drupal, this field is the language as set by the author of the solution.
								When a solution is retrieved from Calaveras, this is the language as set by
								drupal unless that is empty or null, in which case this is set by Calaveras from
								detectLanguage. If language detection fails, it is set by Calaveras to be 'en'.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="issue" type="tns:knowledgeDescriptionType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A description of the issue the solution resolves.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="environment" type="tns:knowledgeDescriptionType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A description of the environment the solution applies to.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="resolution" type="tns:knowledgeDescriptionType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A description of the resolution proposed to resolve the issue.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="rootCause" type="tns:knowledgeDescriptionType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A description of the underlying cause of the issue.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="internalDiagnosticSteps" type="tns:knowledgeDescriptionType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A description of the diagnostic steps a technician should take to diagnose the issue.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="externalDiagnosticSteps" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A description of the diagnostic steps a customer should take to diagnose the issue.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element ref="tns:privateNotes" minOccurs="0" maxOccurs="1" >
						<xs:annotation>
							<xs:documentation>
								The private notes attached to the solution
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="summary" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Summary of the solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="score" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The lucene score for the solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="case" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A case linked to this solution.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="supportNeed" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A support need linked to this solution.  </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="published" type="xs:boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is the solution published?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="hasPublishedRevision" type="xs:boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Does a published revision of this solution exist?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="isLocked" type="xs:boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Is the solution locked?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="lockedBy" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Who locked the solution?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="lockedAt" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>When was the solution locked?  </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="lockExpiresAt" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>When will the lock expire?</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="duplicateOf" type="tns:knowledgeDuplicateLinkType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Links to duplicates of the solution </xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="kcsState" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The state in the KCS lifecycle the solution is currently in. The
								possible states are "wip", "verified", and "unverified".
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="sbrs" type="tns:knowledgeSbrsType">
						<xs:annotation>
							<xs:documentation>SBRs for this solution resource.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="tags" type="tns:tagsType">
						<xs:annotation>
							<xs:documentation>TAGs for this solution resource.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="products" type="tns:knowledgeProductsType">
						<xs:annotation>
							<xs:documentation>Drupal products list for this solution resource.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="LinkedProducts" type="tns:knowledgeLinkedProductsType">
						<xs:annotation>
							<xs:documentation>
								linked products list for this solution Sresource. This list is calculated from a Math, that 
								calculatges if linked case's product can be quanitatively make to this list.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="productFamily" type="tns:knowledgeProductFamilyType">
						<xs:annotation>
							<xs:documentation>This is the families of products to 
								which the solution belongs to.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="ModerationState" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The moderation state the solution is currently in.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="caseCount" type="xs:int" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The count of cases linked to this solution.
								This only appears
								in search results and recommendations.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="kcsProductCount" type="xs:int" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								The count of cases having the same product as that of 
								solution's product or linked product field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="abstract" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								This is a short summary of the Solution
								suitable for display as part of search results. If a particular
								solution
								does not have this field set; then one is automatically
								created by Calaveras.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="extract" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								This field is only set when the solution is
								part of search results (or recommendations). This is an excerpt
								from
								the solution which contains the section(s) of the solution
								that best indicate why this solution matched the
								search query.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="explanation" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Lucene search result explanation of why and how well this document matches the search query.
								This field is only set when the solution is part of search results (or
								recommendations).  This field only is present when the 'explain=true' search query
								parameter is used.  This field is only for debugging purposes.
								See 'extract' for a more generally useful field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="detectedLanguage" type="xs:string" minOccurs="0" maxOccurs="1">
